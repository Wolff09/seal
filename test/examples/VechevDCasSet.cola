#name "Vechev DCAS Set"
#cavespec "set_spec.cav"
// #cave 'prover_opts rem_sorted = "CC_.Head.next";'
#cave 'prover_opts imp_var = "CC_.Head.next";'
#cavebound "key"
#instrumentation "object"


struct Node {
	data_t val;
	Node* next;
}

Node* Head;
Node* Tail;

extern void retire(Node* ptr);
extern void protect1(Node* ptr);
extern void protect2(Node* ptr);


void init() {
	Tail = malloc;
	Tail->next = NULL;
	Tail->val = MAX_VAL;
	Head = malloc;
	Head->next = Tail;
	Head->val = MIN_VAL;
}


bool contains(data_t key) {
	Node* pred, curr;
	data_t k;
	Node* tmpNode;
	bool result;

	curr = Head;
	atomic {
		protect2(curr);
		assert(active(curr));
	}
	k = curr->val;

	while (k < key) {
		pred = curr;
		protect1(pred);

		curr = pred->next;
		protect2(curr);

		choose {
			atomic {
				// assume(curr == pred->next);
				tmpNode = pred->next;
				assume(curr == tmpNode);
			}

			k = curr->val;
		}{
			curr = Head;
			atomic {
				protect2(curr);
				assert(active(curr));
			}
		}
	}

	/*** locate done ***/

	if (k == key) {
		result = true;
	} else {
		result = false;
	}

	return result;
}

bool add(data_t key) {
	Node* pred, curr, entry;
	data_t k;
	Node* tmpNode;
	bool result;

	entry = malloc;
	entry->val = key;

	while (true) {
		/*** locate begin ***/
		
		curr = Head;
		atomic {
			protect2(curr);
			assert(active(curr));
		}
		k = curr->val;

		while (k < key) {
			pred = curr;
			protect1(pred);

			curr = pred->next;
			protect2(curr);

			choose {
				atomic {
					// assume(curr == pred->next);
					tmpNode = pred->next;
					assume(curr == tmpNode);
				}

				k = curr->val;
			}{
				curr = Head;
				atomic {
					protect2(curr);
					assert(active(curr));
				}
			}
		}

		/*** locate done ***/

		if (k == key) {
			result = false;
			// retire(entry); // reclaim new node that was not added to the data structure
			break;

		} else {
			entry->next = curr;
			atomic {
				tmpNode = pred->next;
				if (curr == tmpNode) {
					pred->next = entry;
					result = true;
					break;
				}
			}
		}
	}

	return result;
}


bool remove(data_t key) {
	Node* pred, curr, next;
	data_t k;
	Node* tmpNode;
	bool tmpBool;
	bool result;

	while (true) {
		curr = Head;
		k = curr->val;

		while (k < key) {
			pred = curr;
			protect1(pred);

			curr = pred->next;
			protect2(curr);

			choose {
				atomic {
					// assume(curr == pred->next);
					tmpNode = pred->next;
					assume(curr == tmpNode);
				}

				k = curr->val;
			}{
				curr = Head;
			}
		}

		/*** locate done ***/

		if (k > key) {
			result = false;
			break;
		} else {
			next = curr->next;
			atomic {
				// if (curr == pred->next && next == curr->next)
				tmpNode = pred->next;
				tmpBool = false;
				if (curr == tmpNode) {
					tmpNode = curr->next;
					if (next == tmpNode) {
						tmpBool = true;
					}
				}
				if (tmpBool) {
					// curr->next = Head; // Vafeiadis (makes verification fail)
					// curr->next = NULL; // Vechev
					pred->next = next;
					retire(curr);
					result = true;
					break;
				}
			}
		}
	}

	return result;
}
